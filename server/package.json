    "gulp": "5.0.0",
    "httpyac": "^6.15.1",
    "jest": "^29.2.2",
    "mwts": "^1.3.0",
    "mwtsc": "^1.4.0",
    "ts-jest": "^29.0.3",
    "typescript": "~4.8.0"
