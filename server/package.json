{
  "name": "xianbing",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "scripts": {
    "start": "NODE_ENV=production node ./bootstrap.js",
    "dev": "cross-env NODE_ENV=local mwtsc --watch --run @midwayjs/mock/app.js",
    "test": "cross-env NODE_ENV=unittest jest",
    "cov": "jest --coverage",
    "lint": "mwts check",
    "lint:fix": "mwts fix",
    "ci": "npm run cov",
    "build": "mwtsc --cleanOutDir",
    "yac": "node_modules/.bin/httpyac -e dev -o body ./http.rest",
    "lint-staged": "../node_modules/.bin/lint-staged --config ../.commitlintrc.js",
    "logs": "tail -n 50 -f ~/logs/@xianbing/server/midway-app.log",
    "logs:bootstrap": "tail -n 10000 ~/logs/xianbing/midway-app.log | grep [bootstrap]",
    "logs:pm2": "pm2 logs xb",
    "build:bs3": "pnpm --prefix /node_modules/better-sqlite3 build",
    "sqlite3": "sqlite3 ~/sqlite/xianbing.sqlite"
  },
  "dependencies": {
    "@midwayjs/bootstrap": "3.17.1",
    "@midwayjs/core": "3.17.1",
    "@midwayjs/info": "3.17.3",
