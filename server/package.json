    "@midwayjs/socketio": "^3.17.1",
    "@midwayjs/static-file": "3.17.3",
    "@midwayjs/swagger": "3.17.3",
    "@midwayjs/typeorm": "3.17.1",
    "@midwayjs/validate": "3.17.3",
    "@midwayjs/view-ejs": "^3.17.1",
    "@types/better-sqlite3": "7.6.11",
    "ansis": "^3.3.2",
    "better-sqlite3": "11.5.0",
    "crypto-js": "^4.2.0",
    "dayjs": "^1.11.13",
    "dotenv": "^16.4.5",
    "lodash": "^4.17.21",
    "nanoid": "^3.3.7",
    "octokit": "^3.2.1",
    "pako": "^2.1.0",
    "safe-stable-stringify": "^2.5.0",
    "statuses": "^2.0.1",
    "superagent": "^10.1.0",
    "svg-captcha": "^1.4.0",
    "swagger-ui-dist": "^5.17.14",
    "typeorm": "0.3.20",
    "ua-parser-js": "^1.0.39",
    "uuid": "^10.0.0"
